{
	"express": {
		"route": "/IamLegend"
	},
	"specialization": {
		"experimentation": {
			//pattern 1: Specify a root to look for within the default template folder
			// and name of files within that root folder 
			//[root could be optional, if not specified can look directly into default root]
			"foo": {
				"root": "foo",
				"dust": {
					"partialSamples/partial1": "foo_partial1",
					"partialSamples/partial2": "foo_partial2"
					//"partialSamples/clientRender_sample1": "clientRender_sample1"
				}
			}
			/*"bar": {
				"root": "bar",
				"dust": {
					"partialSamples/partial1": "bar_partial1",
					"partialSamples/partial2": "bar_partial2"
				}
			}*/
		},
			//pattern2: just specify locale assume the templates enabled in the dust map
			//will have an equivalent folder structure inside the locale folder as well
		"locales": {
			"es-US": {
				"dust": {
					"partialSamples/partial1": true,
					"partialSamples/partial2": true
				}
			}
		}
	}
		
