{
    "express": {
        "view engine": "dust",
        "views": "path:./public/templates"
    },
    "view engines": {
        "dust": {
            "module": "engine-munger",
            "renderer" : {
                "method": "dust",
                "arguments": [{"cache": false}]
            }
        },
        "js": {
            "module": "adaro",
            "renderer": {
                "method": "js",
                "arguments": [{"cache": false}]
            }
        }
    },
    "specialization": {
        "partialSamples/partial1" : [
            {
                "template" : "bar/bar_partial1",
                "rules" : {
                    "doIt" : true
                }
            }
        ]
    }
}
